{
    "name": "huybopbi/cyborx-redeem-php",
    "description": "CyborX Auto Redeem Tool - PHP Version",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "huybopbi",
            "email": "huybopbi@example.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-session": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "CyborX\\": "src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "prefer-stable": true,
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "serve": "php -S localhost:8000 -t public"
    }
}